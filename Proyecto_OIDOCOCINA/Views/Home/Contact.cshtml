<style>

    .container-contact100 {
        min-height: 75vh;
        /*background-image: url('../Content/Images/map_parallax.jpg') !important;*/
        width: 100%;
        
    }
    .contact100-form-btn {
        background-color: #5cb85c;
    }
    body {
        background-image: url('../Content/Images/map_parallax.jpg') !important;
    }
    .input100{
        min-width: 100%
    }
</style>

<div class="container-contact100">
    <div class="wrap-contact100">
        <form class="contact100-form validate-form">
            <span class="contact100-form-title centered">
                CONTACTA CON NOSOTROS
            </span>
            <div class="wrap-input100 validate-input bg1" data-validate="Introduce un nombre">
                <span class="label-input100">NOMBRE</span>
                <input class="input100" type="text" name="name" required placeholder="Nombre de contacto">
            </div>

            <div class="wrap-input100 validate-input bg1 rs1-wrap-input100" data-validate="Introduce un email correcto">
                <span class="label-input100">EMAIL</span>
                <input class="input100" type="text" name="email" required placeholder="Email de contacto">
            </div>


            <div class="wrap-input100 bg1 rs1-wrap-input100" data-validate="Introduce un asunto">
                <span class="label-input100">ASUNTO</span>
                <input class="input100" type="text" name="text" required placeholder="Motivo de contacto">
            </div>

            <div class="wrap-input100 validate-input bg0 rs1-alert-validate" data-validate="Introduce un mensaje">
                <span class="label-input100">Message</span>
                <textarea class="input100" name="message" required placeholder="Cuerpo del mensaje"></textarea>
            </div>

            <div class="container-contact100-form-btn">
                <button class="contact100-form-btn btn-success">
                    <span>
                        ENVIAR
                    </span>
                </button>
            </div>
        </form>
    </div>
</div>


